<div class="landing-container">
  <div class="container">
    <div class="container-fluid">
      <h2 class= "landing-bar-text">Bars in your area</h2>
      <div class="scrollmenu">
        <div class="row">
          <div class="col col-md-2">
            <div class="card-landing" style="width: 8rem;">
              <img src="https://media-cdn.tripadvisor.com/media/photo-s/16/25/81/c2/our-new-logo.jpg" />
              <div class="card-landing-infos">
                <h2 class="card-landing-name">Outro Lado</h2>
                <p class="card-landing-location">Alfama, Lisboa</p>
              </div>
            </div>
          </div>
          <div class="col col-md-2">
            <div class="card-landing" style="width: 8rem;">
              <img src="data:image/png;base64," />
              <div class="card-landing-infos">
                <h2 class="card-landing-name">Dois Corvos</h2>
                <p class="card-landing-location">Marvila, Lisboa</p>
              </div>
            </div>
          </div>
          <div class="col col-md-2">
            <div class="card-landing" style="width: 8rem;">
              <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSAHImRTTyD1__rienKxv-2V2ynqJ4_sC9tSw&usqp=CAU" />
              <div class="card-landing-infos">
                <h2 class="card-landing-name">Musa</h2>
                <p class="card-landing-location">Marvila, Lisboa</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="container">
    <div class="container-fluid new-bar">
      <div class="landing-titles">
        <h2 class= "landing-bar-text">New bar in town</h2>
        <p><%= link_to "See more", bars_path %></p>
      </div>
      <div class="row">
        <div class="col col-md-3">
          <div class="card-landing">
            <img src="https://www.artmajeur.com/medias/standard/j/o/joaobeja/artwork/3414331_telhdos_de_lisboa.jpg" style="height: 170px;"/>
            <div class="card-landing-infos">
              <h2 class="card-landing-name">8ª Colina</h2>
              <p class="card-landing-location">Marquês de Pombal, Lisboa</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="container">
    <div class="container-fluid new-beer">
      <div class="landing-titles">
        <h2 class= "landing-bar-text">New beer releases</h2>
        <p><%= link_to "See more", beers_path %></p>
      </div>
      <div class="row">
        <div class="col col-md-3">
          <div class="card-landing">
            <img src="https://static.wixstatic.com/media/b25dd5_0c50f6c755a842e2a84a2b91d8a74c5e~mv2.png/v1/fill/w_1201,h_827,al_c/b25dd5_0c50f6c755a842e2a84a2b91d8a74c5e~mv2.png" style="height: 170px;" />
            <div class="card-landing-infos">
              <h2 class="card-landing-name">Frank APA by Musa</h2>
              <p class="card-landing-location">Marquês de Pombal, Lisboa</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
